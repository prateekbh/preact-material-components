{
  "name": "preact-material-components",
  "version": "1.1.4",
  "description": "preact wrapper for \"Material Components for the web\"",
  "module": "index.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js",
    "build:es": "cross-env NODE_ENV=production babel . --out-dir . --extensions \".jsx\" --ignore node_modules,sample && npm run copycss",
    "copycss": "node copyCss.js",
    "prepublish": "npm run test",
    "test": "npm run build && npm run lint",
    "lint": "eslint src",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prateekbh/preact-material-components.git"
  },
  "keywords": [
    "preact",
    "preact components",
    "material components",
    "material design"
  ],
  "contributors": [
    {
      "name": "Prateek Bhatnagar",
      "email": "prateek89born@gmail.com"
    },
    {
      "name": "Stijn de Witt",
      "email": "StijnDeWitt@hotmail.com"
    },
    {
      "name": "Steve Anderson",
      "email": "steve@twindx.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prateekbh/preact-material-components/issues"
  },
  "homepage": "https://github.com/prateekbh/preact-material-components#readme",
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "material-components-web": "0.17.0"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-node-env-inline": "0.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-babili": "0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babili-webpack-plugin": "0.1.2",
    "clean-webpack-plugin": "^0.1.15",
    "cp": "^0.2.0",
    "cross-env": "^5.0.2",
    "eslint": "^4.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-react": "^7.0.0",
    "lint-staged": "^4.0.0",
    "mkdirp": "^0.5.1",
    "pre-commit": "^1.2.2",
    "preact": "^8.2.1",
    "prettier": "^1.3.1",
    "webpack": "^3.0.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "lint-staged": {
    "*/*.{jsx,js}": [
      "prettier --write",
      "git add"
    ],
    "+(index.js|copyCss.js|componentsList.js|MaterialComponent.js)": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "lint-staged"
}
