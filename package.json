{
  "name": "preact-material-components",
  "version": "2.0.0-alpha.1",
  "description": "preact wrapper for \"Material Components for the web\"",
  "module": "index.js",
  "main": "index.js",
  "scripts": {
    "build": "yarn run buildPackages && ts-node scripts/compile.ts",
    "buildPackages": "lerna run build --ignore '*/docs'",
    "watch": "ts-node scripts/compile.ts --watch",
    "lint": "yarn run tslint && yarn run prettier",
    "tslint": "tslint -p tsconfig.base.json",
    "prettier": "prettier -l {,packages/**/*.{json,js}}",
    "lint:fix": "yarn run tslint:fix && yarn run prettier:fix",
    "tslint:fix": "tslint -p tsconfig.base.json --fix",
    "prettier:fix": "prettier -l {,packages/**/*.{json,js}} --write",
    "prepublishOnly": "yarn run build && yarn run test",
    "test": "yarn run build && yarn run test:only",
    "test:only": "yarn run test:packages && yarn run lint",
    "test:packages": "lerna run test --ignore '*/docs'",
    "prepare": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prateekbh/preact-material-components.git"
  },
  "keywords": [
    "preact",
    "preact components",
    "material components",
    "material design"
  ],
  "author": {
    "name": "Prateek Bhatnagar",
    "email": "prateek89born@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prateekbh/preact-material-components/issues"
  },
  "homepage": "https://github.com/prateekbh/preact-material-components#readme",
  "devDependencies": {
    "@types/shelljs": "^0.8.0",
    "clean-webpack-plugin": "^0.1.17",
    "cp": "^0.2.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "lerna": "^3.4.3",
    "libnpx": "^10.2.0",
    "lint-staged": "^7.2.0",
    "preact": "^8.3.1",
    "prettier": "^1.15.3",
    "shelljs": "^0.8.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3"
  }
}
