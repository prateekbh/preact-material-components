{
  "name": "@preact-material-components/docs",
  "version": "2.0.1-alpha.0",
  "description": "preact material component's site",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prateekbh/preact-material-components.git"
  },
  "keywords": [
    "preact",
    "preact components",
    "material components",
    "material design"
  ],
  "author": {
    "name": "Prateek Bhatnagar",
    "email": "prateek89born@gmail.com"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build --template template.html",
    "build:prod": "preact build --template template.html && yarn run copyAssets",
    "copyAssets": "cpx -v '{*.png,manifest.json,prerender-urls.json}' build && cpx -v 'images/**' build/images/",
    "dev": "preact watch --template template.html",
    "test": "cypress run --parallel",
    "test-runner": "node ./test-runner.js",
    "test:travis": "server-test start http-get://localhost:8080/ test-runner"
  },
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prateekbh/preact-material-components/issues"
  },
